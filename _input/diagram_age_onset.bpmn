<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Defs_OnsetResolution" targetNamespace="http://example.com/onset-resolution" exporter="bpmn-js (https://demo.bpmn.io)" exporterVersion="18.6.1">
  <bpmn:process id="Process_OnsetResolution" name="Resolve Onset Ages (ANY &#38; PRIMARY) from C2–C6 + C1 bounds" isExecutable="false">
    <bpmn:startEvent id="Start" name="Start" />
    <bpmn:task id="LoadExcel" name="Load object w/ inconsistencies&#10;(invalid_age_onset_ext_info_w_c2c6)" />
    <bpmn:task id="ComputeBounds" name="Compute Bounds">
      <bpmn:documentation>
        ANY: LB=5; UB=min_adm_age_rec3-1
        PRIMARY(sub): LB=max(5, onset_any_final); UB=min(c1_min_adm_age_by_subs -1, min_adm_age_rec3 -1)
      </bpmn:documentation>
    </bpmn:task>
    <bpmn:subProcess id="ResolveANY" name="Resolve ANY onset&#10;(mode → median)">
      <bpmn:documentation>
        Candidate pool: all *_age_subs_onset* columns.
        Filter to [LB_any, UB_any], 5–80, &lt; min_adm_age_rec3.
        Pick mode (smallest on tie), else median; else unresolved.
        PROC_ANY: ANY=mode[...] / ANY=median[...] / ANY=unresolved.
        FLAG_ANY: UB missing, LB&gt;UB, or no candidate in range.
      </bpmn:documentation>
    </bpmn:subProcess>
    <bpmn:subProcess id="ResolvePRIMARY" name="Resolve PRIMARY onset per&#10;(hash × substance)">
      <bpmn:documentation>
        Candidate pool: c[1-6]_age_prim_subs_onset*_mean_* columns (per standardized substance).
        Filter to [LB_primary, UB_primary], 5–80.
        Enforce PRIMARY ≥ ANY (if ANY exists) — otherwise NA + conflict flag.
        Pick mode (smallest on tie), else median; else unresolved.
        PROC_PRIMARY: PRIMARY=mode[...] / PRIMARY=median[...] / PRIMARY=unresolved.
        FLAG_PRIMARY: UB missing, LB&gt;UB, primary&lt;any, or no candidate in range.
      </bpmn:documentation>
    </bpmn:subProcess>
    <bpmn:task id="Compose" name="Compose PROC &#38; FLAG + resolved values">
      <bpmn:documentation>
        PROC = PROC_ANY | PROC_PRIMARY
        FLAG = FLAG_ANY ; FLAG_PRIMARY
        Outputs:
          onset_any_final, LB_any, UB_any
          onset_primary_final, LB_primary, UB_primary
      </bpmn:documentation>
    </bpmn:task>
    <bpmn:task id="QASummary" name="QA Summary (counts)">
      <bpmn:documentation>
        any_resolved, any_unresolved,
        primary_resolved, primary_unresolved
      </bpmn:documentation>
    </bpmn:task>
    <bpmn:endEvent id="End" name="End" />
    <bpmn:sequenceFlow id="f1" sourceRef="Start" targetRef="LoadExcel" />
    <bpmn:sequenceFlow id="f2" sourceRef="LoadExcel" targetRef="ComputeBounds" />
    <bpmn:sequenceFlow id="f3" sourceRef="ComputeBounds" targetRef="ResolveANY" />
    <bpmn:sequenceFlow id="f4" sourceRef="ResolveANY" targetRef="ResolvePRIMARY" />
    <bpmn:sequenceFlow id="f5" sourceRef="ResolvePRIMARY" targetRef="Compose" />
    <bpmn:sequenceFlow id="f6" sourceRef="Compose" targetRef="QASummary" />
    <bpmn:sequenceFlow id="f7" sourceRef="QASummary" targetRef="End" />
    <bpmn:textAnnotation id="AnnInputs">
      <bpmn:text>Inputs from table:
• *_age_subs_onset* (ANY candidates)
• c[1-6]_age_prim_subs_onset*_mean_* (PRIMARY candidates)
• min_adm_age_rec3 (minimum age at admission)
• c1_min_adm_age_by_subs_* (one col per substance)</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="AnnRules">
      <bpmn:text>Global rules:
• Ages in [5,80]
• ANY value &lt; minimum age at admission
• PRIMARY(sub) &lt; min(c1_min_adm_age_by_subs, min_adm_age_rec3)
• PRIMARY ≥ ANY (if ANY exists)</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="a1" sourceRef="LoadExcel" targetRef="AnnInputs" />
    <bpmn:association id="a2" sourceRef="ComputeBounds" targetRef="AnnRules" />
  </bpmn:process>
